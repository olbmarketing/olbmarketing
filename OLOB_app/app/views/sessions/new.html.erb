<div class="container">
    <div class="row">
        <div class="col-sm-6 col-md-4 col-md-offset-4">
			<h1 class="text-center login-title">OLOB Marketing Login</h1>
			<div class="form-signin">
				<div class = "account-wall">
					<%= form_tag '/login' do %>
				  	Email: <%= text_field_tag :email, '',:class => "form-control" %> <br />
				  	Password: <%= password_field_tag :password, '',:class => "form-control"%> <br />
				  	<%= submit_tag "Submit" %>
					<% end %>
				</div>
			</div>			
		</div>
	</div>
</div>